{
  "name": "wxt-starter",
  "description": "manifest.json description",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "format": "biome format ./entrypoints",
    "format-fix": "biome format --write ./entrypoints",
    "lint": "biome lint ./entrypoints",
    "lint-fix": "biome check --apply ./entrypoints",
    "check": "pnpm run compile && pnpm run lint && pnpm run format",
    "check-fix": "pnpm run lint-fix && pnpm run format-fix"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.1",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.3.3",
    "wxt": "^0.17.0"
  }
}
